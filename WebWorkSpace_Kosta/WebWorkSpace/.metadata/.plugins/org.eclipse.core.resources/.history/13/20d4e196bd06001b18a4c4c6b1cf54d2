<title>Insert title here</title>

</head>
<body>
	<h3>JavaScript 변수의 개념 - var & let & const</h3>
	<script>
		//자바스크립트 문법 적용 - JavaScript 영역
		var a;
		var b = 101;
		var c = "희정";
		d = 3.14 // var 키워드 생략 

		//브라우저에 출력
		document.write("<h4>");
		document.write("a = " + a + ", " + (typeof a) + "<br>");
		document.write("b = " + b + ", " + (typeof b) + "<br>");
		document.write("c = " + c + ", " + (typeof c) + "<br>");
		document.write("d = " + d + ", " + (typeof d) + "<br>");

		//값 변경 
		a = new String("배고프다");
		b = "뭐 먹지";
		c = 20;
		d = new Array(3);

		document.write("<hr>");
		document.write("a = " + a + ", " + (typeof a) + "<br>");
		document.write("b = " + b + ", " + (typeof b) + "<br>");
		document.write("c = " + c + ", " + (typeof c) + "<br>");
		document.write("d = " + d + ", " + (typeof d) + "<br>");

		document.write("</h4>");

		//함수 작성
		function test(a, b) {
			
		}
		
		//함수 호출 
		test(4,2);
		test();
		test(1,3,5,5);
		test("A","B");
		
	</script>

</body>
