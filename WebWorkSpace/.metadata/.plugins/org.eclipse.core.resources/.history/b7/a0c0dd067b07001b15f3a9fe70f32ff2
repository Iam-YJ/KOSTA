<meta charset="EUC-KR">
<title>Insert title here</title>
</head>
<body>
	<h3>Math 관련 함수</h3>
	<script>
		document.write("Math.random() = " + Math.random() + "<br>"); //0.0 ~ 0.999

		var nansu = Math.floor(Math.random() * 5) + 1; //1~5
		document.write("nansu = " + nansu + "<br>");

		document.write("<h3> Array Object 사용하기 <p>");
		//배열선언
		var arr1 = [ 10, 20, 30, 40 ];
		var arr2 = new Array("유진", "나영", "효리");

		var arr3 = [ 10, "서울", true, "대구", 3.14 ];
		var arr4 = new Array(5); //5개의 공간만 생성

		//함수 호출
		printArray(arr1);
		printArray(arr2);
		printArray(arr3);
		printArray(arr4);

		//배열의 데이터 출력하는 기능
		function printArray(objArr) {
			document.write("obj type = " + (typeof objArr) + "<br>");
			document.write("obj length = " + (objArr.length) + "<br>");

			for (var i = 0; i < objArr.length; i++) {
				document.write("objArr [" + i + "] = " + objArr[i] + "<br>");
			}
			document.write("<hr color='red'>");
		}

		document.write("</h3>");
	</script>
</body>
</html>